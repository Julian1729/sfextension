{
  "name": "DM Salesforce SF Extension",
  "version": "1.0",
  "description": "Log into client's wordpress site with Username and Password from Salesforce.",
  "manifest_version": 2,
  "icons": {
    "16": "images/icons/16x16.png",
    "32": "images/icons/32x32.png",
    "48": "images/icons/48x48.png",
    "128": "images/icons/128x128.png"
  },
  "background": {
    "scripts": ["./js/background.js"],
    "persistant:": true
  },
  "browser_action": {
    "default_popup": "./popup.html"
  },
  "permissions": ["tabs", "http://*/", "https://*/", "storage"]
}
